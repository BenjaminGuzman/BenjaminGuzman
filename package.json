{
  "name": "benjaminguzman",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tailwind": "tailwindcss build --input src/tailwind.scss --config tailwind.config.js --output src/tailwind.css",
    "start": "ng serve & npm run tailwind -- --watch",
    "build": "npm run tailwind && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run benjaminguzman:serve-ssr",
    "serve:ssr": "node dist/benjaminguzman/server/main.js",
    "build:ssr": "ng build && ng run benjaminguzman:server",
    "prerender": "npm run tailwind && ng run benjaminguzman:prerender && cp robots.txt dist/benjaminguzman/browser"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.1",
    "@angular/common": "~13.2.1",
    "@angular/compiler": "~13.2.1",
    "@angular/core": "~13.2.1",
    "@angular/forms": "~13.2.1",
    "@angular/platform-browser": "~13.2.1",
    "@angular/platform-browser-dynamic": "~13.2.1",
    "@angular/platform-server": "~13.2.1",
    "@angular/router": "~13.2.1",
    "@nguniversal/express-engine": "^13.0.2",
    "@supabase/supabase-js": "^1.29.1",
    "express": "^4.15.2",
    "rxjs": "7.5.2",
    "swiper": "^8.0.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.1",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^16.11.22",
    "@types/offscreencanvas": "^2019.6.4",
    "autoprefixer": "^10.4.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.5",
    "typescript": "~4.5.5"
  }
}
